{
    "name": "Butter",
    "homepage": "http://butterproject.org/",
    "bugs": "https://github.com/butterproject/butter-desktop/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/butterproject/butter-desktop.git"
    },
    "license": "GPL-3.0",
    "main": "app://host/src/app/index.html",
    "version": "0.3.8-5a",
    "releaseName": "There's nothing on TV",
    "scripts": {
        "postinstall": "node_modules/.bin/bower install --config.interactive=false && grunt setup",
        "postupdate": "node_modules/.bin/bower update --config.interactive=false && grunt setup",
        "test": "grunt --verbose",
        "start": "nw"
    },
    "window": {
        "title": "Butter",
        "icon": "src/app/images/icon.png",
        "toolbar": false,
        "frame": false,
        "min_width": 960,
        "min_height": 520,
        "resizable": true,
        "show": false,
        "position": "center"
    },
    "dependencies": {
