+	File "..\..\package.json"
+
+SectionEnd
+
+Section ; Shortcuts
+
+	SetOutPath "$INSTDIR"
+	File /oname=app.ico "..\..\images\popcorntime.ico"
+
+	;Working Directory
+	SetOutPath "$INSTDIR"
+
